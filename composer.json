  "keywords": [
    "impersonate",
    "symfony-style",
    "laravel"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Alexander Zhuravlev",
      "email": "scif-1986@yandex.ru"
    }
  ],
  "type": "library",
  "require": {
    "php": ">=7.0",
    "laravel/framework": "~5.3",
    "laravel/browser-kit-testing": "^1.0"
  },
  "require-dev": {
    "laravel/laravel": "~5.3",
    "phpunit/phpunit": "^5.7",
    "mockery/mockery": "0.9.*"
